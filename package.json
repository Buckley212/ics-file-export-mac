    "calendar"
  ],
  "author": "Itai Noam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Buckley212/ics-file-export-mac/issues"
  },
  "homepage": "https://github.com/Buckley212/ics-file-export-mac#readme",
  "devDependencies": {
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^26.0.19",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "blob": "^0.1.0",
    "file-saver": "^2.0.5"
